"UPDATE MemberMealCoupen mmc SET mmc.status = :NEW_STATUS, " 
				+ "mmc.createdUpdated.updatedBy = :UPDATED_BY, mmc.createdUpdated.updatedAt = :UPDATED_AT  " 
				+ "WHERE mmc.status = :OLD_STATUS AND mmc.member IN ("
				+ ")";

